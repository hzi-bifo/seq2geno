#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null && pwd )"

# Here the working environment is activated.

env=$1
snakefile=$2
configfile=$3
workdir=$4
cores=$5
dryrun=$6
notemp=$7

source activate $env

$DIR'/main.internal.py' \
  --snakefile $snakefile \
  --configfile $configfile \
  --workdir $workdir \
  --cores $cores\
  --dryrun $dryrun \
  --notemp $notemp 
